{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src"],
    "exclude": ["node_modules", "src/server/express/routes/api"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["jsdoc-babel"],
  "babel": {
    "presets": ["react"],
    "plugins": [
      "transform-object-rest-spread",
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "sourceType": "module",
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/app/",
    "template": "./node_modules/minami"
  }
}
