    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "fs-readdir-recursive": "^1.1.0",
    "ignore-styles": "^5.0.1",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "jsx-loader": "^0.13.2",
    "mocha": "^5.0.1",
    "mocha-jenkins-reporter": "^0.3.10",
