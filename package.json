  "scripts": {
    "start:server": "webpack --config ./config/webpack.config.dev.server.js",
    "start:client": "webpack-dev-server --config ./config/webpack.config.dev.client.js",
    "start": "rm -rf ./build && cross-env BABEL_ENV=development NODE_ENV=development run-p start:client start:server",
    "build:server": "webpack --config ./config/webpack.config.prod.server.js",
    "build:client": "webpack --config ./config/webpack.config.prod.client.js",
    "build": "rm -rf ./build && cross-env BABEL_ENV=production NODE_ENV=production run-s build:server build:client",
    "build-qa": "rm -rf ./build && cross-env BABEL_ENV=production NODE_ENV=qa run-s build:server build:client",
    "build-dev": "rm -rf ./build && cross-env BABEL_ENV=development NODE_ENV=development run-s build:server build:client",
    "pretest": "cross-env MONGODB_VERSION=\"3.0.12\" mongodb-runner start --port=28018",
    "test": "cross-env REPORTER=spec node config/testScript.js",
    "test:xunit": "cross-env REPORTER=mocha-jenkins-reporter JUNIT_REPORT_PATH=test_results.xml node config/testScript.js",
    "test:jenkins": "run-s pretest test:xunit posttest",
    "posttest": "mongodb-runner stop --port=28018",
    "test:watch": "npm test -- --watch"
  },
  "eslintConfig": {
    "parserOptions": {
      "experimentalDecorators": true
    },
    "extends": "react-app",
    "env": {
      "mocha": true
    }
  },
  "eslintIgnore": [
    "**/*.test.tpl.js",
    "**/*.test.js.snap"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "7.2.3",
